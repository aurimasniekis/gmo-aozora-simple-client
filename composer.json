{
  "name": "aurimasniekis/gmo-aozora-simple-client",
  "description": "A simple client for accessing your bank information from https://gmo-aozora.com/",
  "keywords": [
    "gmo",
    "aozora",
    "client"
  ],
  "homepage": "https://github.com/aurimasniekis/gmo-aozora-simple-client",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Aurimas Niekis",
      "email": "aurimas@niekis.lt",
      "homepage": "https://aurimas.niekis.lt"
    }
  ],
  "support": {
    "issues": "https://github.com/aurimasniekis/gmo-aozora-simple-client",
    "source": "https://github.com/aurimasniekis/gmo-aozora-simple-client"
  },
  "require": {
    "php": "^7.4||^8.0",
    "aurimasniekis/gmo-aozora-client": "^1.0",
    "kriswallsmith/buzz": "^1.1",
    "nyholm/psr7": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "AurimasNiekis\\GmoAozoraSimpleClient\\": "src"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2"
  },
  "scripts": {
    "cs-check": "php-cs-fixer fix --dry-run --diff --diff-format udiff",
    "cs-fix": "php-cs-fixer fix"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
